# git-specific aliases
alias gcsm='git commit -S -m'
alias gpnew='git push -u origin HEAD'
alias gcnv='git commit --no-verify -m'
alias gro1='git rebase --onto origin/master HEAD~1'
alias gro2='git rebase --onto origin/master HEAD~2'
alias gro3='git rebase --onto origin/master HEAD~3'
alias gro4='git rebase --onto origin/master HEAD~4'
alias gro5='git rebase --onto origin/master HEAD~5'
alias gfp='git fetch && git pull'